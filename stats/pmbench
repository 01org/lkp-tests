#!/usr/bin/env ruby

res = 0.0
histo_r = Array.new(24)
histo_w = Array.new(24)
sum_r = 0.0
sum_w = 0.0
mode = nil
READ = :read
WRITE = :write

loop do
  line = STDIN.gets
  break unless line
  case line
  when /^Read/
    mode = READ
  when /^Write/
    mode = WRITE
  when /^  Page latency during benchmark \(inc. gen\): (\d+\.\d+) us \(\d+ clks\)/
    latency = $1
    puts "latency.ns.average: #{latency}"
  when /^2\^\((\d+),(\d+)\) ns: (\d+)/
    end_n = $2.to_i
    cnt = $3.to_i
    p = if end_n < 32
          end_n - 8
        else
          end_n - 9
        end
    if mode == READ
      sum_r += cnt
      histo_r[p] = cnt
    else
      sum_w += cnt
      histo_w[p] = cnt
    end
  end
end

def format_power2(num)
  p = 0
  unit = ['', 'K', 'M', 'G']
  while num > 9
    num -= 10
    p += 1
  end
  (1 << num).to_s + unit[p]
end

def gen_output(mode, i, res)
  if i.zero?
    puts "#{mode}.latency.ns.0-256%: #{res}"
  elsif i == 23
    puts "#{mode}.latency.ns.1G-inf%: #{res}"
  else
    puts "#{mode}.latency.ns.#{format_power2(i + 7)}-#{format_power2(i + 8)}%: #{res}"
  end
end

[[histo_r, sum_r, READ], [histo_w, sum_w, WRITE]].each do |histo, sum, modes|
  (0..23).each do |i|
    res = histo[i] / sum * 100
    gen_output(modes, i, res)
  end
end
