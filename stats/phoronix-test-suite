#!/usr/bin/ruby

test_id = -1
while line = STDIN.gets
	case line.chomp!
	when /pts\/(\S+)-[0-9.]+/
		test_name = $1
	when /Average: ([0-9.]+) (.*)/
		value = $1
		unit = $2
		test_id += 1
		unit = unit.downcase.gsub(' ', '_').gsub('/', '_')
		puts "#{test_name}.#{test_id}.#{unit}: #{value}"
	end
end
