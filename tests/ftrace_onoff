#!/bin/sh

cd /sys/kernel/debug/tracing || exit

: ${runtime:=300}

do_test()
{
	for tracer in $(cat available_tracers)
	do
		note "echo $tracer > current_tracer"
		      echo $tracer > current_tracer

		note "echo 1 > tracing_on"
		      echo 1 > tracing_on

		note "echo 0 > tracing_on"
		      echo 0 > tracing_on
	done
}

. $LKP_SRC/lib/runtime_loop.sh
runtime_loop
